<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
  <sim>
    <description>Aeritalia F-104S Starfighter</description>
    <long-description>
      Aeritalia F-104 Starfighter for the FlightGear Flight Simulator.
      The Aeritalia F-104S Starfighter was a licensed production Italian version
      of the Lockheed F-104 Starfighter, which served in the Italian Air Force, 
      and was its mainstay from the late 1960s until the beginning of the 
      21st century. The F-104S also served in the Turkish Air Force until 
      the mid-1990s. 
      The F-104S was the final development of the Starfighter line.
    </long-description>
    <aircraft-version>2023-02-27-16</aircraft-version>

    <rating>
      <FDM type="int">1</FDM>
      <systems type="int">1</systems>
      <cockpit type="int">1</cockpit>
      <model type="int">1</model>
    </rating>

    <authors>
      <author n="0">
        <name>RAF1</name>
        <description>3D Model, Research</description>
        <nick>RAF1</nick>
    </author>

      <author n="1">
        <name>Megaf</name>
        <description>Project Leader, Q/A, Graphics</description>
        <nick>Megaf</nick>
      </author>
    </authors>

    <maintainers>
      <maintainer n="0">
        <name>Megaf</name>
        <email>mmegaf@gmail.com</email>
      </maintainer>
    </maintainers>

    <primary-set type="bool">true</primary-set>

    <tags>
      <tag n="0">Lockheed</tag>
      <tag n="1">F-104</tag>
      <tag n="2">Jet</tag>
      <tag n="3">Fighter</tag>
      <tag n="4">Turbojet</tag>
      <tag n="5">Military</tag>
    </tags>

    <urls>
      <home-page>https://forum.flightgear.org/viewtopic.php?f=4&amp;t=41282</home-page>
      <support>https://github.com/Megaf/F-104/discussions/categories/support</support>
      <wikipedia>https://en.wikipedia.org/wiki/Aeritalia_F-104S_Starfighter</wikipedia>
      <code-repository>https://github.com/Megaf/F-104/</code-repository>
    </urls>

    <flight-model>jsb</flight-model>
    <aero>F-104S</aero>

    <model>
      <path>Models/F-104S.xml</path>
    </model>

    <menubar>
      <default>
        <menu n="128">
          <label>F-104</label>
          <enabled type="bool">true</enabled>
          <item>
            <label>Engine Start</label>
            <binding>
              <command>dialog-show</command>
              <dialog-name>enginestart</dialog-name>
            </binding>
          </item>
        </menu>
      </default>
    </menubar>

  </sim>
</PropertyList>
